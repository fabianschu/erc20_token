      "declaration": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "noEmit": true,
      "skipLibCheck": true,
      "strict": true,
      "target": "es2020",
